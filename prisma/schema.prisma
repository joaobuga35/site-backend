// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Occupation {
  id         String  @id @default(uuid()) @map("id") @db.Uuid
  name       String
  office     String
  image      String?
  linkedin   String?
  discord    String?
  created_at String
}
